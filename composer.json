{
    "name": "aliamini78/module-builder",
    "version": "1.0.0",
    "description": "Package for making modules in Laravel.",
    "keywords": ["laravel", "module", "modular"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ali Amini",
            "email": "a.amini78@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "illuminate/console": ">=10.0",
        "illuminate/filesystem": ">=10.0",
        "illuminate/support": ">=10.0",
        "illuminate/validation": ">=10.0",
        "symfony/console": ">=6.0"
    },
    "autoload": {
        "psr-4": {
            "aliamini78\\ModuleBuilder\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "aliamini78\\ModuleBuilder\\ModuleBuilderServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
